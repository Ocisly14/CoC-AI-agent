{
  "name": "coc-multiagent",
  "version": "0.1.0",
  "description": "LangGraph-based multi-agent starter written in TypeScript.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "build:swc": "pnpm rebuild better-sqlite3 && swc src -d dist",
    "build:tsc": "pnpm rebuild better-sqlite3 && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=coc:* tsx src/index.ts",
    "chat": "tsx client/server.ts",
    "clean": "rm -rf dist node_modules/.cache",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": ["langgraph", "langchain", "multi-agent", "typescript"],
  "author": "CoC AI",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.29",
    "@langchain/langgraph": "^0.0.32",
    "@langchain/openai": "^0.3.4",
    "better-sqlite3": "^11.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "langchain": "^0.3.8",
    "mammoth": "^1.8.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.3.100",
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "@types/pdf-parse": "^1.1.4",
    "@vitest/ui": "^2.1.8",
    "cross-env": "^7.0.3",
    "only-allow": "^1.2.1",
    "turbo": "^2.4.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.7"
}
